{
  "name": "ui",
  "version": "1.0.0",
  "description": "UI",
  "main": "dist/demo/index.html",
  "keywords": [
    "ui",
    "design",
    "system",
    "component"
  ],
  "author": "Mark Morcos",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "clean:demo": "rm -rf dist/demo",
    "clean:storybook": "rm -rf dist/storybook",
    "start": "concurrently 'yarn start:demo' 'yarn start:storybook'",
    "start:demo": "parcel src/index.html",
    "start:storybook": "start-storybook -p 6006 --ci --quiet",
    "build": "yarn clean && concurrently 'yarn build:demo' 'yarn build:storybook'",
    "build:demo": "yarn clean:demo && parcel build --dist-dir dist/demo --public-url /ui/demo src/index.html",
    "build:storybook": "yarn clean:storybook && build-storybook -o dist/storybook --quiet",
    "test": "jest src",
    "test:e2e": "cypress run --quiet"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-docs": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.11",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "cypress": "^8.1.0",
    "jest": "^27.0.6",
    "jsdom": "^16.6.0",
    "parcel": "next",
    "parcel-bundler": "^1.12.5",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  }
}
