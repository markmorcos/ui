{
  "name": "ui",
  "version": "1.0.0",
  "description": "UI",
  "main": "dist/ui/index.html",
  "keywords": [
    "ui",
    "design",
    "system",
    "component"
  ],
  "author": "Mark Morcos",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "clean:ui": "rm -rf dist/ui",
    "clean:storybook": "rm -rf dist/storybook",
    "start": "concurrently 'yarn start:ui' 'yarn start:storybook'",
    "start:ui": "parcel src/index.html",
    "start:storybook": "start-storybook -p 6006 --ci --quiet",
    "build": "yarn clean && concurrently 'yarn build:ui' 'yarn build:storybook'",
    "build:ui": "yarn clean:ui && parcel build --dist-dir dist/ui --public-url https://ui-fe.s3.eu-central-1.amazonaws.com src/index.html",
    "build:storybook": "yarn clean:storybook && build-storybook -o dist/storybook --quiet",
    "test": "jest",
    "deploy": "aws s3 sync dist/storybook s3://ui-fe --acl=public-read"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-docs": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "jest": "^27.0.6",
    "parcel": "next",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
