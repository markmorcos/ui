{
  "name": "@markmorcos/ui",
  "version": "1.0.3",
  "description": "UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/markmorcos/ui.git"
  },
  "main": "dist/lib/index.js",
  "files": [
    "dist/lib/**/*"
  ],
  "keywords": [
    "ui",
    "design",
    "system",
    "component"
  ],
  "author": "Mark Morcos",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "clean:lib": "rimraf dist/lib",
    "clean:storybook": "rimraf dist/storybook",
    "start": "start-storybook -p 6006 --ci --quiet",
    "build": "yarn clean && concurrently 'yarn build:lib' 'yarn build:storybook'",
    "build:lib": "yarn clean:lib && tsc",
    "build:storybook": "yarn clean:storybook && build-storybook -o dist/storybook --quiet",
    "test": "jest src"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-docs": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.11",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "jest": "^27.0.6",
    "jsdom": "^16.6.0",
    "parcel": "next",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  }
}
